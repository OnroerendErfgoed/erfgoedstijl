    <div class="large-3 columns">

	<div class="panel fl-block-search">
		<h3>Zoeken</h3>

		<form class="form-search" method="GET" action="/besluiten">
			
				<input name="query" type="text" class="span2 search-query" value="" />
	
				<select name="sort" class="span2">
					<option  selected="sorteervolgorde">sorteervolgorde</option>
					<option  value="-datum_ondertekening">datum ondertekening</option>
					<option value="onderwerp">onderwerp</option>
					<option  value="id">id</option>				
				</select>
		
	
				<h5>beschermingstypes</h5>

				<ul id="facet_beschermingstypes" class="nodisk small-list" >
				
					<li><label class="checkbox" for="chkbx_beschermingstypes_SD">
						<input 	type="checkbox" name="beschermingstypes" value="SD" id="chkbx_beschermingstypes_SD" /> stads- of dorpsgezicht <small>(1)</small>
						</label>
					</li>

				</ul>
				
				<h5>provincies</h5>

				<ul id="facet_provincies" class="nodisk small-list" >

						<li>
							<label 	class="checkbox" 
									for="chkbx_provincies_10000">
								<input 	type="checkbox"
										name="provincies"
										value="10000"
										id="chkbx_provincies_10000" /> Antwerpen <small>(1)</small>
							</label>
						</li>
				
				</ul>
				
		
				<h5>rechtsgevolgen</h5>

				<ul id="facet_rechtsgevolgen" class="nodisk small-list">
			
					<li>
						<label 	class="checkbox" for="chkbx_rechtsgevolgen_21" style="">
						<input 	type="checkbox" name="rechtsgevolgen" value="21" id="chkbx_rechtsgevolgen_21" /> besluiten tot ontwerp van lijst <small>(1)</small>
						</label>
					</li>
					
					
				    <li>
						<label 	class="checkbox" for="chkbx_rechtsgevolgen_15">
						<input 	type="checkbox" name="rechtsgevolgen" value="15" id="chkbx_rechtsgevolgen_15" /> opheffingsbesluiten <small>(1)</small>
						</label>
				     </li>	
				</ul>
				

			
		
		
		<h5>ondertekening</h5>
		
		<ul id="facet_ondertekening" class="nodisk small-list">
			
				<li>
					<label class="checkbox" 
						   for="chkbx_ondertekening_2015">
						<input type="checkbox"
							   name="jaar_ondertekening"
							   value="2015"
							   id="chkbw_ondertekening_2015"
							   
							   />
						2015 <small>(1)</small>
					</label>
				</li>
		
		</ul>
				
				<script type="text/javascript">
					var hidden = $('#facet_ondertekening li:gt(9)').hide();
					if (hidden.length > 0) {
						$('#facet_ondertekening')
							.after('<p class="facet_expander" id="expand_facet_ondertekening">Meer ...</p>');
						$('#expand_facet_ondertekening').on('click', function(evt) {
							if ($('#expand_facet_ondertekening').hasClass('facet_expanded')) {
								$('#facet_ondertekening li:gt(9)').slideUp('slow');
								$('#expand_facet_ondertekening').removeClass('facet_expanded');
								$('#expand_facet_ondertekening').html('Meer ...');
							} else {
								$('#facet_ondertekening li:gt(9)').slideDown('slow');
								$('#expand_facet_ondertekening').addClass('facet_expanded');
								$('#expand_facet_ondertekening').html('Minder ...');
							}
						});
					}
				</script>
				
		
		<h5>personen</h5>

				<ul id="facet_personen" class="nodisk small-list">
					<li>
						<label 	class="checkbox" for="chkbx_personen_34">
							<input 	type="checkbox" name="personen" value="34" id="chkbx_personen_34"/> Prins Karel <small>(1)</small>
						</label>
					</li>
				</ul>
				
				<script type="text/javascript">
					var hidden = $('#facet_personen li:gt(9)').hide();
					if (hidden.length > 0) {
						$('#facet_personen')
							.after('<p class="facet_expander" id="expand_facet_personen">Meer ...</p>');
						$('#expand_facet_personen').on('click', function(evt) {
							if ($('#expand_facet_personen').hasClass('facet_expanded')) {
								$('#facet_personen li:gt(9)').slideUp('slow');
								$('#expand_facet_personen').removeClass('facet_expanded');
								$('#expand_facet_personen').html('Meer ...');
							} else {
								$('#facet_personen li:gt(9)').slideDown('slow');
								$('#expand_facet_personen').addClass('facet_expanded');
								$('#expand_facet_personen').html('Minder ...');
							}
						});
					}
				</script>
				
		
			
	
				<h5>uitvaardigers</h5>
				<ul id="facet_uitvaardigers" class="nodisk small-list">
		
					<li>
						<label 	class="checkbox" for="chkbx_uitvaardigers_8">
							<input 	type="checkbox" name="uitvaardigers" value="8" id="chkbx_uitvaardigers_8" /> besluiten Vlaamse Regering <small>(1)</small>
						</label>
					</li>
			
				</ul>

			
      
				<select name="status" class="span2">
					<option value=" ">beheer status</option>
					<option  value="25">klaar voor activatie</option>
					<option  value="10">klad</option>
					<option  value="75">actief</option>						
				</select>
        

				<input type="submit" class="button expand" value="Zoeken" />
				<input type="reset" id="reset-zoeken" class="button expand tiny" value="Wissen" />
				<script type="text/javascript">
					$('#reset-zoeken').on('click', function(evt) {
						$('.fl-block-search form input[type="text"]').attr('value', '');
						$('.fl-block-search form select[name="sort"]').val('-datum_ondertekening');
						$('.fl-block-search form select[name="status"]').val(' ');
						$('.fl-block-search form input[type="checkbox"]').removeAttr('checked');
					});
				</script>
		
		</form>
