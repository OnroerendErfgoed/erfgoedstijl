<!-- a little bit of jQuery to spice things up -->
<script type="text/javascript">
  $(function(){ 
        $('li.more > span').click(function(){
            if($(this).hasClass('open')){
                $(this).removeClass('open').parent().parent().find('.toggle').each(function(index){
                    $(this).delay(100*index).hide(300);
                }); 
            }else{
                $(this).addClass('open').parent().parent().find('.toggle').each(function(index){
                    $(this).delay(100*index).show(300);
                });             
            }
        });
    });
</script>






